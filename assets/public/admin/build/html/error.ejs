		/*footer*/

		.footer {
			position: fixed;
			width: 100%;
			left: 0px;
			bottom: 0px;
			height: 60px;
			line-height: 60px;
			overflow: hidden;
			z-index: 2;
			text-align: center;
			color: #fff;
		}

		.footer p {
			text-align: center;
			font-size: 12px;
			line-height: 60px;
			color: #fff;
		}

		.right {
			float: right;
		}

		.left {
			float: left;
		}

		.margin_b1 {
			margin-bottom: 5px;
		}
