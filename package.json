    "helmet": "^3.13.0",
    "lodash": "^4.17.10",
    "lowdb": "^1.0.0",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "request": "^2.88.0",
    "telegraf": "^3.22.2",
    "unzip": "^0.1.11",
    "uuid": "^3.3.2",
    "winston": "^3.0.0"
  }
