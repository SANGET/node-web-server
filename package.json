{
  "name": "orion-web-server",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "babel": "babel-node ./src/index.js --presets es2015",
    "start": "PLATFORM=v6Prev nodemon --watch src/ --exec npm run babel",
    "build": "rm -Rf ./lib && babel src -d lib",
    "_deploy": "NODE_ENV=production nodemon --ignore assets/ --exec npm run babel",
    "init": "mkdir -p ./assets/manager/build",
    "fe:server": "npm run init; NODE_ENV=production pm2 start ./lib/servers/fe.server.js --name='fe:server'",
    "babel:fe": "nodemon --watch src/ --exec DEBUG=express:* babel-node ./src/servers/fe.server.js --presets es2015",
    "babel:i": "nodemon --watch src/ --exec DEBUG=express:* PLATFORM=v6Prev babel-node ./src/index.js --presets es2015"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SANGET/orion-web-server.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "debug": "^3.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3"
  },
  "author": "Alex",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SANGET/orion-web-server/issues"
  },
  "homepage": "https://github.com/SANGET/orion-web-server#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-fileupload": "^0.4.0",
    "extract-zip": "^1.6.7",
    "fs-extra": "^7.0.0",
    "helmet": "^3.13.0",
    "lodash": "^4.17.10",
    "morgan": "^1.9.0",
    "request": "^2.88.0",
    "telegraf": "^3.22.2",
    "winston": "^3.0.0"
  }
}
